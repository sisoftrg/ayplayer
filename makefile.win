#MAKE = wmake

all: help
help:
	echo type 'make real' or 'make adlib' for making.

real:
	$(MAKE) -f src/makefile.win SNDOUT=-dLPTPORT=0x378 all
adlib:
	$(MAKE) -f src/makefile.win SNDOUT=-dADLIB all

install:
	$(MAKE) -f src/makefile.win install
clean:
	$(MAKE) -f src/makefile.win clean
